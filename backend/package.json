{
  "name": "transitsng-backend",
  "version": "1.0.0",
  "description": "Backend for TransiSNG transportation platform",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "migrate": "npx sequelize-cli db:migrate --env production",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "migrate:create": "npx sequelize-cli migration:generate --name",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo:all",
    "seed:undo:all": "npx sequelize-cli db:seed:undo:all",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start:prod": "NODE_ENV=production node server.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.4",
    "axios": "^1.5.0",
    "stripe": "^12.18.0",
    "twilio": "^4.18.0",
    "express-rate-limit": "^6.8.1",
    "helmet": "^7.0.0",
    "express-validator": "^7.0.1",
    "compression": "^1.7.4",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.4",
    "supertest": "^6.3.3",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1"
  },
  "keywords": [
    "transportation",
    "logistics",
    "backend",
    "nodejs",
    "express",
    "sequelize",
    "postgresql"
  ],
  "author": "TransiSNG Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}